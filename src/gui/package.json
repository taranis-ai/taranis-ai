{
  "name": "@taranis-ai/taranis-ui",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/taranis-ai/taranis-ai.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm run patch_config",
    "build:docker": "vite build --base='/__TARANIS_BASE_PATH__/' && pnpm run patch_config",
    "preview": "vite preview",
    "lint": "eslint --fix src",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "lint_and_format": "pnpm run lint && pnpm run format",
    "patch_config": "node ./extras/patch_config_json.js",
    "test": "pnpm run lint && pnpm run format"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-json": "^6.0.1",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@mdi/font": "^7.4.47",
    "@sentry/vue": "^8.35.0",
    "@vuepic/vue-datepicker": "^8.8.1",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "codemirror": "^6.0.1",
    "js-base64": "^3.7.7",
    "object-hash": "^3.0.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^3.2.3",
    "sse.js": "^2.5.0",
    "uuid": "^10.0.0",
    "vue": "^3.5.12",
    "vue-chartjs": "^5.3.1",
    "vue-codemirror6": "^1.3.4",
    "vue-dompurify-html": "^5.1.0",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.4.5",
    "vue-use-hotkeys": "^0.4.0",
    "vuetify": "^3.7.3",
    "zipson": "^0.2.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "sass": "^1.80.4",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-vuetify": "^2.0.4",
    "vue-tsc": "^2.1.8"
  }
}
