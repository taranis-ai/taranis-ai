{
  "name": "taranis-ui",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run patch_config",
    "build:docker": "vite build --base='/__TARANIS_BASE_PATH__/' && npm run patch_config",
    "preview": "vite preview",
    "prepack": "node ./extras/update_version.js",
    "postpack": "node ./extras/remove_version.js",
    "lint": "eslint --ext .js,.vue --fix src",
    "package": "npm run prepack && npm pack && npm run postpack",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "lint_and_format": "npm run lint && npm run format",
    "patch_config": "node ./extras/patch_config_json.js",
    "test": "npm run lint && npm run format"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-json": "^6.0.1",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@mdi/font": "^7.4.47",
    "@sentry/vue": "^8.28.0",
    "@vuepic/vue-datepicker": "^8.8.1",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.7.7",
    "codemirror": "^6.0.1",
    "js-base64": "^3.7.7",
    "object-hash": "^3.0.0",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^3.2.3",
    "sse.js": "^2.5.0",
    "uuid": "^10.0.0",
    "vue": "^3.4.31",
    "vue-chartjs": "^5.3.1",
    "vue-codemirror6": "^1.3.4",
    "vue-dompurify-html": "^5.1.0",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3",
    "vue-use-hotkeys": "^0.4.0",
    "vuetify": "^3.7.1",
    "zipson": "^0.2.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-vuetify": "^2.0.4",
    "vue-tsc": "^2.1.4"
  }
}
