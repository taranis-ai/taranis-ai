{
  "name": "taranis-ui",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run patch_config",
    "preview": "vite preview",
    "prepack": "node ./extras/update_version.js",
    "postpack": "node ./extras/remove_version.js",
    "lint": "eslint --ext .js,.vue --fix src",
    "package": "npm run prepack && npm pack && npm run postpack",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "lint_and_format": "npm run lint && npm run format",
    "patch_config": "node ./extras/patch_config_json.js",
    "test": "npm run lint && npm run format"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.8",
    "@codemirror/lang-json": "^6.0.1",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@mdi/font": "^7.4.47",
    "@sentry/vue": "^7.102.1",
    "@vuepic/vue-datepicker": "^8.1.1",
    "axios": "^1.6.7",
    "codemirror": "^6.0.1",
    "js-base64": "^3.7.7",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.20",
    "vue-chartjs": "^5.3.0",
    "vue-codemirror": "^6.1.1",
    "vue-dompurify-html": "^5.0.1",
    "vue-i18n": "^9.9.1",
    "vue-pdf-embed": "^2.0.2",
    "vue-router": "^4.3.0",
    "vue-shortkey": "^3.1.7",
    "vue3-keypress": "^4.0.1",
    "vuetify": "^3.5.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-vuetify": "^2.0.1",
    "vue-tsc": "^1.8.27"
  }
}
